  .ltabs-tabs-container{
    position: relative;
    .tabs-menu{
      position: relative;
      @media (min-width: 569px){
        position: absolute;
        #{$right}: 0px;
        top: 0px;
        z-index: 8;
      }
      @media (min-width: 569px){
        padding: 0;
        .tabs-menu_btn{
          display : none;
        }
