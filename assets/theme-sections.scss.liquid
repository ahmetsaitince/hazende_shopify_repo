/*============================================================================
========================== Widget Book ==================================
==============================================================================*/

.ss-book-wrapper {
  margin-bottom: 101px;
  @media (max-width: 991px){
    margin-bottom: 30px;
  }
  .ss-book-inner{
    display: inline-block;
    width: 100%;
  }
  .ss-book-left{
    float: $left;
    width: 50%;
    @media (max-width: 991px){
      float: $left;
      width: 100%;
    }
    .ss-book-img{
      margin-top: 73px;
      margin-bottom: 15px;
    }
    .ss-book-form{
      position: relative;
      .form-success{
        position: absolute;
        bottom: -39px;
        left: 0;
        width: 100%;
      }
    }

  }
  .ss-book-right{
    float: $right;
    width: 50%;
    @media (max-width: 991px){
      float: $left;
      width: 100%;
    }
    img{
      width: 100%;
    }
  }
}
.ss-book-wrapper img {
  margin: 0 auto;
}
.ss-book-wrapper .ss-book-inner .ss-book-top {
  text-align: center;	
}
.ss-book-wrapper .ss-book-heading {
  margin-top: 10px;
}
.ss-book-wrapper .ss-book-heading h2 {
  color: #000;
  text-transform: uppercase;
  font-weight: bold;
  font-size: 30px;
  margin-bottom: 52px;
}
.ss-book-wrapper .ss-book-time-open {
  margin-top: 30px;
  .time-open{
    font-size: 14px;
    font-weight: 300;
    color: #808080;
  }
}
.ss-book-wrapper .ss-book-time-open .book-phone {
  font-size: 200%;
  margin-top: 10px;
  font-weight: 300;
  margin-bottom: 64px;
}
.ss-book-wrapper .ss-book-form {
  width: 58%;
  margin: 0 auto;
  margin-top: 30px;
}
.ss-book-wrapper .ss-book-form select,
.ss-book-wrapper .ss-book-form input.date,
.ss-book-wrapper .ss-book-form input.time {
  background: transparent;
  border: 0;    
  width: 95%;
}
.ss-book-wrapper .ss-book-form select:focus,
.ss-book-wrapper .ss-book-form input.date:focus,
.ss-book-wrapper .ss-book-form input.time:focus {
  border: 0;
  outline: 0;
}
.ss-book-wrapper .ss-book-form input[type="submit"] {
  background-color: #000;
  color: #fff;
  width: 100%;
  text-align: center;
  padding: 10px 0;
  font-weight: bold;
  font-size: 14px;
  &:hover{
    background: $main-color;
  }
}
.ss-book-wrapper .ss-book-form .book-field {
  background-color: #eee;
  padding: side-values(10px 30px 12px 64px);
  margin-bottom: 10px;
  position: relative;
}
.ss-book-wrapper .ss-book-form .book-field .book-icon {
  position: absolute;
  #{$left}: 38px;
  color: #000;
  font-size: 16px;
}
.ss-book-wrapper .ss-book-form .book-field .book-select {
  position: absolute;
  top: 11px;
  #{$right}: 30px;
  font-weight: bold;
  font-size: 18px;
  @include if-rtl{
    i{
      transform: rotate(180deg);
    }
  }
}
@media (max-width: 1500px){
  .ss-book-wrapper{
    .ss-book-left .ss-book-img{
      margin-top: 10px;
      margin-bottom: 15px;
    }
    .ss-book-heading{
      margin-top: 10px;
      h2{
        font-size: 26px;
        margin-bottom: 0px;
      }
    }
    .ss-book-time-open{
      margin-top: 7px;
    }
    .ss-book-time-open .book-phone{
      font-size: 150%;
      margin-top: 10px;
      margin-bottom: 20px;
    }
    .ss-book-form .book-field{
      padding: side-values(5px 30px 7px 64px);
    }
    .ss-book-form input[type="submit"]{
      padding: 6px 0;
    }
    .ss-book-form .book-field .book-select{
          top: 6px;
    }
  }
}
@media (max-width: 991px){
  .ss-book-wrapper{
    .ss-book-left .ss-book-img{
      margin-top: 20px;
    }
    .ss-book-form{
      width: 85%;
    }
  }
}