/*============================================================================
========================= SECCTION QUICKVIEWS ================================
==============================================================================*/
.template-index{
  .owl-nav{
    > div{
      @include transform(translateY(-50%));
      position: absolute;
      top: 50%;
      width: 26px;
      height: 20px;
      line-height: 20px;
      text-align: center;
      margin: 0;
      cursor: pointer;
      display: inline-block;
      @include transition(all 400ms ease-in-out);
      position: absolute;
      z-index: 2;
      i{
        display: none;
      }
      &.owl-prev{
        left: 0px;
        background: url({{ 'left-arrow1.png' | asset_url }}) no-repeat center;
        @include if-rtl{
          float: left; 
        }
        @media (min-width: 1300px){
          left: -25px;
        }
      }
      &.owl-next{
        right: 0px;

        background: url({{ 'right-arrow1.png' | asset_url }}) no-repeat center;
        @media (min-width: 1300px){
          right: -25px;
        }
      }
      &:hover{
        i.fa{
          color: #fff;
        }
      }
    }
  }
}
.owl-style_dot{
  .owl-dots{
    position: absolute;
    top: -45px;
    #{$right}: -5px;
    .owl-dot{
      width: 8px;
      height: 8px;
      border-radius: 8px;
      background-color: #222;
      margin: 0 5px;
      display: inline-block;
      float: $left;
      opacity: 1;
      transition: all 0.2s ease-in-out 0s; 
      &.active {
        width: 30px;
        background-color: $main-color;
      }
    }
  }
}
.owl-style2{
  .owl-nav{
    position: absolute;
    top: -24px;
    #{$right}: 0px;
    > div{
      background-color: transparent;
      border: 1px solid #eee;
      color: #999;
      height: 32px;
      width: 24px;
      margin: 0;
      position: static; 
      line-height: 30px;
      @media screen and (-webkit-min-device-pixel-ratio:0) { 
        line-height: 28px;
      }
      &:hover{
        border-color: $main-color; 
      }
      &.owl-prev {
        border-right: none;
      }
      &.owl-next{
        border-left: none;
      }
    }
  }
}
