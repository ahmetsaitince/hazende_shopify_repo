          &.active {
            background-color: #161619;
          }
        }
      }
    }
  }
  .owl-nav>div {
    top : 50%;
    @media (max-width : 767px) {
      display : none;
    }
    &.owl-prev {
      left : -45px;
    }
    &.owl-next {
      right : -45px;
    }
    .fa {
      background : rgba(255, 255, 255, 0.1);
    }
  }
  @media (min-width : 992px) and (max-width : 1500px) {
    .client-carousel{
      padding-top: 0;

    }
    .bg-image .testimonial .info .client-image{
      margin-bottom: 30px;
    }
    .bg-image .testimonial .client-quote{
      margin: 0 10% 10px 10%;
    }
    .bg-image .testimonial .info .client-info{
      margin: 10px 0 10px 0;
    }
    .bg-image .testimonial .owl-dots{
      margin-top: 20px;
    }
    .bg-image .testimonial .client-quote:before{
      #{$left}: -15px;
    }
  }
}

