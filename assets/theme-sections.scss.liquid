@mixin owl-nav(){
  .owl-nav{ 
    div{
      width: 30px;
      height: 30px;
      line-height: 30px;
      background-color: #c1c1c1;
      -webkit-transition: all 0.3s ease-in-out 0s;
      -moz-transition: all 0.3s ease-in-out 0s;
      transition: all 0.3s ease-in-out 0s;
      top: 50%;
      text-align: center;
      @include transform(translateY(-50%));
      position: absolute;
      color: #fff;

      &:hover{background: $main-color;}
      &.owl-prev{
        left: -15px;
        float: left;
        background-color: #c1c1c1;
        &:hover{
          background-color: $main-color;
          opacity: 1;
        }
      }
      &.owl-next{
        right: -15px;
        float: right;
        background-color: #c1c1c1;
        &:hover{
          background-color: $main-color;
          opacity: 1;
        }
      }

    }    
  }
}
@mixin owl-controls(){
  .owl-nav {
    div.owl-prev, div.owl-next{
      top: -65px;
      position: absolute;
      height: 30px;
      width: 30px;
      line-height: 30px;
      border: none;
      opacity: 1;
      background: #c1c1c1;
      font-size: 18px;
      margin: 0;
      padding: 0;
      color: #fff;
      text-align: center;
      @include transition (all 0.2s ease-in-out 0s);
      @media (max-width: 568px){
        top: -55px;
        height: 24px;
        width: 24px;
        line-height: 24px;
      }
      &:before{
        display: none;
      }
      &:hover{
        background-color: $main-color;
      }
    }
    div.owl-prev{
      #{$left}: auto; 
      #{$right}: 32px;
      &:hover {
        background-position: 0px 0;
      }

    }
    div.owl-next{
      #{$left}: auto; 
      #{$right}: 0;
    }

  }
}
